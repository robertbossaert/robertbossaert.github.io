{
  "name": "robertbossaert-com",
  "description": "My personal website",
  "scripts": {
    "serve": "rm -rf docs && npx @11ty/eleventy --serve --quiet & npx tailwindcss -i ./src/tailwind.css -o ./docs/styles.css --watch",
    "build": "rm -rf docs && ELEVENTY_ENV=production npx @11ty/eleventy && npx tailwindcss -i ./src/tailwind.css -o ./docs/styles.css --minify"
  },
  "keywords": [
    "eleventy",
    "tailwindcss",
    "starter"
  ],
  "author": "Robert Bossaert",
  "license": "MIT",
  "type": "module",
  "browserslist": "defaults, not ie <= 11",
  "devDependencies": {
    "@11ty/eleventy": "^3.0.0",
    "tailwindcss": "^3.4.14"
  },
  "dependencies": {
    "@11ty/eleventy-img": "^5.0.0",
    "@11ty/eleventy-navigation": "^0.3.5"
  }
}
